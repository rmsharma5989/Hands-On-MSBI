  <DataSets>
    <DataSet Name="dset_AmountDetails">
      <Query>
        <DataSourceName>sd_AdventureWorksFullDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pTerritoryID">
            <Value>=Parameters!pTerritoryID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@pCategoryID">
            <Value>=Parameters!pCategoryID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@pSubCategoryID">
            <Value>=Parameters!pSubCategoryID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@pProductID">
            <Value>=Parameters!pProductID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT t.TerritoryID,ps.ProductCategoryID,psc.ProductSubcategoryID,p.ProductID,sum(s.subTotal) [Sub Total], sum(s.TaxAmt) [Tax Amount],sum(s.Freight) [Freight Amount]
FROM SALES.SalesOrderHeader S(NOLOCK) 
INNER JOIN SALES.SalesOrderDetail SD (NOLOCK) ON S.SalesOrderID = SD.SalesOrderID
INNER JOIN SALES.SalesTerritory T ON T.TerritoryID = S.TerritoryID
inner join Production.Product p on p.ProductID = sd.ProductID
inner join Production.ProductSubcategory psc on psc.ProductSubcategoryID = p.ProductSubcategoryID
inner join Production.ProductCategory ps on ps.ProductCategoryID = psc.ProductCategoryID
group by t.TerritoryID,ps.ProductCategoryID,psc.ProductSubcategoryID,p.ProductID
having t.TerritoryID = @pTerritoryID
and ps.ProductCategoryID = @pCategoryID
AND PSC.ProductSubcategoryID = @pSubCategoryID
AND p.ProductID = @pProductID</CommandText>
      </Query>
      <Fields>
        <Field Name="TerritoryID">
          <DataField>TerritoryID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ProductID">
          <DataField>ProductID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ProductCategoryID">
          <DataField>ProductCategoryID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Sub_Total">
          <DataField>Sub Total</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ProductSubcategoryID">
          <DataField>ProductSubcategoryID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Tax_Amount">
          <DataField>Tax Amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Freight_Amount">
          <DataField>Freight Amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
